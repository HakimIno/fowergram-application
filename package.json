    "expo-media-library": "^17.0.6",
    "expo-status-bar": "~2.0.1",
    "expo-system-ui": "~4.0.9",
    "expo-video": "~2.0.6",
    "lottie-react-native": "7.1.0",
    "moti": "^0.29.0",
    "react": "18.3.1",
    "react-native": "0.76.9",
