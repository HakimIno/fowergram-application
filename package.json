    "@react-navigation/native": "^7.0.14",
    "@react-navigation/native-stack": "^7.2.0",
    "@react-navigation/stack": "^6.3.29",
    "@reduxjs/toolkit": "^2.2.5",
    "@shopify/flash-list": "1.7.3",
    "@shopify/react-native-skia": "1.5.0",
    "@tanstack/react-query": "^5.45.1",
