  },
  "private": true,
  "expo": {
    "install": {
      "exclude": [
        "expo-updates",
        "expo-splash-screen"
      ]
    },
    "doctor": {
      "reactNativeDirectoryCheck": {
        "exclude": [
          "@faker-js/faker",
          "react-native-portalize",
          "react-native-pinchable"
        ],
        "listUnknownPackages": false
      }
    }
  }
