{
  "name": "flowergram",
  "version": "1.0.1",
  "scripts": {
    "start": "expo start --dev-client",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "clean": "rm -rf node_modules && yarn cache clean && yarn install",
    "prebuild:android": "npx expo prebuild --platform android",
    "prebuild:ios": "npx expo prebuild --platform ios",
    "prebuild": "npx expo prebuild",
    "prebuild:clean": "rm -rf android ios && npx expo prebuild",
    "build:dev:android": "eas build --profile development --platform android",
    "build:dev:android:local": "eas build --profile development --platform android --local",
    "build:dev:ios": "eas build --profile development --platform ios",
    "build:prod:android": "eas build --profile production --platform android",
    "build:prod:android:local": "eas build --profile production --platform android --local",
    "build:preview:android": "eas build --profile preview --platform android",
    "build:preview:ios": "eas build --profile preview --platform ios",
    "build:preview:ios:local": "eas build --profile preview --platform ios --local",
    "build:preview:android:local": "eas build --profile preview --platform android --local",
    "build:preview": "eas build --profile preview",
    "build:preview:local": "eas build --profile preview --local",
    "build:prod:ios": "eas build --profile production --platform ios",
    "submit:android": "eas submit -p android",
    "submit:ios": "eas submit -p ios",
    "update:dev": "eas update --channel development",
    "update:prod": "eas update --channel production",
    "start:lan": "REACT_NATIVE_PACKAGER_HOSTNAME='192.168.95.23' expo start --lan",
    "start:fast": "expo start --dev-client --no-dev --minify",
    "android:fast": "expo run:android --no-dev --minify",
    "ios:fast": "expo run:ios --no-dev --minify"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.0.3",
    "@faker-js/faker": "^9.2.0",
    "@kuss/react-native-mesh-gradient": "^0.2.0",
    "@react-native-community/slider": "4.5.6",
    "@react-navigation/bottom-tabs": "^7.2.0",
    "@react-navigation/material-top-tabs": "^7.1.0",
    "@react-navigation/native": "^7.0.14",
    "@react-navigation/native-stack": "^7.2.0",
    "@react-navigation/stack": "^6.3.29",
    "@shopify/flash-list": "1.7.6",
    "@shopify/react-native-skia": "2.0.0-next.4",
    "@tanstack/react-query": "^5.45.1",
    "axios": "^1.7.2",
    "expo": "53.0.9",
    "expo-av": "~15.1.4",
    "expo-blur": "~14.1.4",
    "expo-brightness": "~13.1.4",
    "expo-build-properties": "~0.14.6",
    "expo-camera": "~16.1.6",
    "expo-dev-client": "~5.1.8",
    "expo-font": "~13.3.1",
    "expo-haptics": "~14.1.4",
    "expo-image": "~2.1.7",
    "expo-image-picker": "~16.1.4",
    "expo-linear-gradient": "~14.1.4",
    "expo-media-library": "~17.1.6",
    "expo-splash-screen": "~0.29.0",
    "expo-status-bar": "~2.2.3",
    "expo-system-ui": "~5.0.7",
    "expo-video": "~2.1.9",
    "immer": "^10.1.1",
    "lottie-react-native": "7.2.2",
    "moti": "^0.29.0",
    "react": "19.0.0",
    "react-native": "0.79.2",
    "react-native-dotenv": "^3.4.11",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-keyboard-controller": "^1.17.1",
    "react-native-mmkv": "^3.2.0",
    "react-native-pager-view": "6.7.1",
    "react-native-pinchable": "^0.2.1",
    "react-native-portalize": "^1.0.7",
    "react-native-reanimated": "~3.17.4",
    "react-native-redash": "^18.1.3",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.10.0",
    "react-native-svg": "15.11.2",
    "react-native-tab-view": "^4.0.5",
    "react-native-vision-camera": "^4.6.4",
    "react-native-worklets-core": "^1.5.0",
    "reanimated-color-picker": "^4.0.1",
    "resolve-from": "^5.0.0",
    "use-debounce": "^10.0.4",
    "zod": "^3.24.3",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/plugin-transform-template-literals": "^7.24.1",
    "@react-native-community/cli": "^18.0.0",
    "@types/lodash": "^4.17.13",
    "@types/react": "~19.0.10",
    "lodash": "^4.17.21",
    "typescript": "~5.8.3"
  },
  "resolutions": {
    "yargs": "^17",
    "@expo/config-plugins": "~10.0.0",
    "@expo/prebuild-config": "~9.0.0"
  },
  "private": true,
  "expo": {
    "install": {
      "exclude": [
        "expo-updates",
        "expo-splash-screen"
      ]
    },
    "doctor": {
      "reactNativeDirectoryCheck": {
        "exclude": [
          "@faker-js/faker",
          "react-native-portalize",
          "react-native-pinchable"
        ],
        "listUnknownPackages": false
      }
    }
  }
}
